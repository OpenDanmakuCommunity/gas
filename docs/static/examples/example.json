{
    "objects": {
        "ExampleTextObject": {
            "type": "Text",
            "content": "This is some sample text",
            "font": {
                "size": 12,
                "family": "SimHei",
                "decoration": 0,
            },
            "position": {
                "x": 0,
                "y": 0,
                "anchor": [0, 0],
                "axis": 0
            }
        },
        "ExampleButtonObject": {
            "type": "Button",
            "content": "This is some sample button text",

        },
        "ExampleSpriteObject": {
            "type": "SVGSprite",
        },
        "ExampleFrameObject": {
            "type": "Frame",
            "size": {
                "width": 100,
                "height": 100,
            }
        },
    },
    "layers": [
        {
            "name": "default",
            "components": ["ExampleTextObject", "ExampleSpriteObject", "ExampleFrameObject"]
        },
        {
            "name": "Extra Layer 1",
            "components": [],
            "mixing": "default"
        }
    ],
    "animation": {
        "groups": {
            "group1": ["ExampleFrameObject", "ExampleSprite"]
        },
        "anchors": [
            {
                "time": 1000,
                "objects": {
                    "group1": {
                      "linear": {
                        "position": {
                            "x": 200,
                            "y": 300
                        }
                      },
                      "none": {
                        "content": "foo!"
                      }
                    }
                }
            }
        ]
    },
    "metadata": {
        "stage": {
          "width": 640,
          "height": 480,
          "autoScale": true
        },
        "interactions": {
          "sampleClickInteraction": {
            "type": "hit-target",
            "action": "open",
            "parameters": {
              "location": "video://vid100/0",
              "window": "replace",
              "pause": true,
            },
            "style": {
              "accessible": true,
              "outline": "always",
              "hover": {
                "color": 0,
                "background": 0,
              },
              "touchPolicy": "default"
            }
          }
        },
        "layers": {
            "orphans": "hide",
            "defaultMixing": "none"
        },
        "animation": {
            "duration": 6000,
            "legacy": false
        }
    }
}
